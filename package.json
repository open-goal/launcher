{
  "name": "opengoal-launcher",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauricon": "tauricon",
    "package": "node ./scripts/prepare-sidecars.mjs",
    "lint": "npx prettier --check .",
    "format": "npx prettier --write .",
    "prepare-release-config": "node ./scripts/merge-config.js",
    "update-release-meta": "node ./scripts/update-release-meta.mjs",
    "release": "npm run prepare-release-config && npm run tauri build -- --config ./.tauri/release-config.combined.json",
    "release-id": "node ./scripts/get-release-id.mjs"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@iconify/svelte": "^3.1.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "@popperjs/core": "^2.11.6",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tauri-apps/cli": "^1.2.3",
    "@tauri-apps/tauricon": "github:tauri-apps/tauricon",
    "@tsconfig/svelte": "^3.0.0",
    "ansi-to-span": "^0.0.1",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "escape-html": "^1.0.3",
    "execa": "^7.0.0",
    "flowbite": "^1.6.3",
    "flowbite-svelte": "^0.32.1",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "svelte-navigator": "^3.2.2"
  }
}
