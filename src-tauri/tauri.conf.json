{
  "bundle": {
    "active": true,
    "targets": [
      "msi",
      "app",
      "dmg",
      "deb",
      "appimage"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "macOS": {
      "signingIdentity": "-"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "createUpdaterArtifacts": false
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000/"
  },
  "productName": "OpenGOAL-Launcher",
  "mainBinaryName": "OpenGOAL-Launcher",
  "version": "2.7.1",
  "identifier": "OpenGOAL-Launcher",
  "app": {
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "capabilities": [
        "desktop-capability"
      ]
    },
    "windows": [
      {
        "title": "OpenGOAL Launcher - Splash",
        "label": "splashscreen",
        "width": 200,
        "height": 300,
        "center": true,
        "decorations": false,
        "transparent": true,
        "resizable": false,
        "fullscreen": false,
        "url": "./src/splash/index.html",
        "visible": true,
        "focus": true,
        "useHttpsScheme": true
      }
    ]
  }
}